    "/api/define-fields/{project_id}": {
      "post": {
        "summary": "Define Fields",
        "description": "Define fields to extract for a project.\nExpects [{'name': 'Effective Date', 'description': 'The start date...'}]\nFor now, this just maps to 'questions' for generating answers later, \nor could trigger generation immediately if we wanted.",
        "operationId": "define_fields_api_define_fields__project_id__post",
        "parameters": [
          {
            "name": "project_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Project Id"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
